{
  "name": "wagomu-no-akibako",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build && pagefind --site dist",
    "preview": "astro preview",
    "astro": "astro",
    "markuplint": "markuplint \"./src/**/*.astro\"",
    "lint:check": "eslint src",
    "lint:fix": "eslint --fix src",
    "precommit": "pnpm build",
    "format": "prettier --write --plugin-search-dir=. src",
    "format:check": "prettier --check --plugin-search-dir=. "
  },
  "husky": {
    "hooks": {
      "pre-commit": "precommit"
    }
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.3",
    "@astrojs/mdx": "4.0.0-beta.1",
    "@astrojs/rss": "^4.0.7",
    "@astrojs/sitemap": "^3.1.6",
    "@markuplint/astro-parser": "^4.6.5",
    "astro": "5.0.0-beta.2",
    "cheerio": "1.0.0-rc.12",
    "husky": "^9.0.11",
    "markuplint": "^3.15.0",
    "mdx": "^0.3.1",
    "prettier": "^3.3.2",
    "prettier-plugin-astro": "^0.11.1",
    "remark-breaks": "^4.0.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@pagefind/default-ui": "^1.1.1",
    "@playform/compress": "^0.1.1",
    "@playform/inline": "^0.1.0",
    "astro-google-fonts-optimizer": "^0.2.2",
    "pagefind": "^1.1.1",
    "unist-util-visit": "5.0.0"
  }
}
